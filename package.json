{
  "name": "expo-simple-ui-apps",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --dev-client",
    "build": "eas build --profile development --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|(?!axios)/|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)|@expo/vector-icons/FontAwesome|expo-linear-gradient"
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "^6.0.0",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@types/lodash": "^4.14.195",
    "@types/react-native-snap-carousel": "^3.8.5",
    "axios": "^1.4.0",
    "deprecated-react-native-prop-types": "^4.1.0",
    "expo": "~48.0.18",
    "expo-dev-client": "~2.2.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-speech": "~11.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "fetch-mock": "^9.11.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "lodash": "^4.17.21",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "13.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.0.14",
    "axios-mock-adapter": "^1.21.5",
    "patch-package": "^7.0.1",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.4"
  },
  "private": true
}
